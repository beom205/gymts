<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mt.gmts.mapper.SampleMapper">
	
	<select id="getEmployee" parameterType="int" resultType="string">
		SELECT first_name 
		  FROM employees
		 WHERE employee_id = #{employeeId} 
	</select>
</mapper>